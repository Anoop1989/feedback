<p id="notice"><%= notice %></p>

<h1>Listing Feedbacks</h1>

<table class="table">
  <thead>
    <tr>
      <th></th>
      <th>Meeting with</th>
      <th>Meeting happened</th>
      <th>Was useful</th>
      <th>Potential revenue</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.id %></td>
        <td><%= feedback.meeting_happened %></td>
        <td><%= feedback.was_useful %></td>
        <td><%= feedback.potential_revenue %></td>
        <td><%= feedback.comments %></td>
        <td><%= link_to 'Show', feedback %></td>
        <td><%= link_to 'Edit', edit_feedback_path(feedback) %></td>
        <td><%= link_to 'Destroy', feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Feedback', new_feedback_path %>
