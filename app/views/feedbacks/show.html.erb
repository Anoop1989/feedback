<div class="row">
  <div class="col-md-4 col-md-offset-4">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">Feedback</h3>
      </div>
      <div class="panel-body">
        <p id="notice"><%= notice %></p>
        <p>
          <strong>Meeting with:</strong>
          <%= @feedback.meeting_with %>
        </p>

        <p>
          <strong>Meeting happened:</strong>
          <%= @feedback.meeting_happened %>
        </p>

        <p>
          <strong>Was useful:</strong>
          <%= @feedback.was_useful %>
        </p>

        <p>
          <strong>Potential revenue:</strong>
          <%= @feedback.potential_revenue %>
        </p>

        <p>
          <strong>Comments:</strong>
          <%= @feedback.comments %>
        </p>
      </div>
      <div class="panel-footer text-right">
        <%= link_to 'Edit', edit_feedback_path(@feedback), class: 'btn btn-xs btn-primary'  %> |
      <%= link_to 'Back', feedbacks_path, class: 'btn btn-xs btn-primary'  %>
      </div>
    </div>
  </div>
</div>
